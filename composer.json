{
    "name": "ahmedtarboush/papyrus-docs",
    "description": "A native, reflection-based documentation generator for Laravel. Turns your code into a living codex.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "AhmedTarboush\\PapyrusDocs\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AhmedTarboush\\PapyrusDocs\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AhmedTarboush\\PapyrusDocs\\PapyrusServiceProvider"
            ],
            "aliases": {
                "Papyrus": "AhmedTarboush\\PapyrusDocs\\Facades\\Papyrus"
            }
        }
    },
    "authors": [
        {
            "name": "Ahmed Tarboush",
            "email": "ahmedaymantarboush@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "homepage": "https://github.com/ahmedaymantarboush/papyrus-docs",
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/routing": "^10.0|^11.0|^12.0",
        "illuminate/view": "^10.0|^11.0|^12.0",
        "phpdocumentor/reflection-docblock": "^5.3"
    },
    "require-dev": {
        "orchestra/testbench": "^10.9",
        "pestphp/pest": "^3.8",
        "pestphp/pest-plugin-laravel": "^3.2"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "prefer-stable": true
}
